	<DataTemplate
		x:Key="{DataTemplateKey {x:Type local:RectangleViewModel}}"
		DataType="{x:Type local:RectangleViewModel}">
		<Grid>
			<Rectangle
				Width="{Binding Borderwidth}"
				Height="{Binding Borderheight}">
				<Shape.Fill>
					<DrawingBrush>
						<DrawingBrush.Drawing>
							<GeometryDrawing
								Brush="{x:Null}"
								Geometry="{Binding OuterRectStyle}" />
						</DrawingBrush.Drawing>
					</DrawingBrush>
				</Shape.Fill>
			</Rectangle>
			<Grid
				HorizontalAlignment="Center"
				VerticalAlignment="Center">
				<Rectangle
					Width="{Binding Outerwidth}"
					Height="{Binding Outerheight}"
					Tag="{Binding KeyTag}">
					<Shape.Fill>
						<DrawingBrush>
							<DrawingBrush.Drawing>
								<GeometryDrawing
									Brush="{Binding Strokecolor, Converter={StaticResource colorToBrushConverter}}"
									Geometry="{Binding OuterRectStyle}" />
							</DrawingBrush.Drawing>
						</DrawingBrush>
					</Shape.Fill>
				</Rectangle>
				<Grid
					HorizontalAlignment="Center"
					VerticalAlignment="Center">
					<Rectangle
						Width="{Binding Innerwidth}"
						Height="{Binding Innerheight}">
						<Shape.Fill>
							<DrawingBrush>
								<DrawingBrush.Drawing>
									<GeometryDrawing
										Brush="{Binding Color, Converter={StaticResource colorToBrushConverter}}"
										Geometry="{Binding InnerRectStyle}" />
								</DrawingBrush.Drawing>
							</DrawingBrush>
						</Shape.Fill>
					</Rectangle>
					<Grid
						HorizontalAlignment="Center"
						VerticalAlignment="Center">
						<Canvas
							Left="0"
							Top="0"
							Width="{Binding Innerwidth}"
							Height="{Binding Innerheight}">
							<Rectangle
								Width="{Binding Innerfontwidth}"
								Height="{Binding Innerfontheight}"
								Canvas.Left="{Binding Innerfontwidth_Canvasleft}"
								Canvas.Top="{Binding Innerfontwidth_Canvastop}">
								<Shape.Fill>
									<DrawingBrush>
										<DrawingBrush.Drawing>
											<GeometryDrawing
												Brush="{Binding Fontcolor, Converter={StaticResource colorToBrushConverter}}"
												Geometry="{Binding InnerStyle}" />
										</DrawingBrush.Drawing>
									</DrawingBrush>
								</Shape.Fill>
							</Rectangle>
						</Canvas>
					</Grid>
				</Grid>
			</Grid>
		</Grid>
	</DataTemplate>
