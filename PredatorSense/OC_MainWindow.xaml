<Window
	x:Class="PredatorSense.OC_MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Name="mainWindow"
	Title="PredatorSense "
	Height="760"
	Width="1320"
	WindowStyle="None"
	AllowsTransparency="True"
	UseLayoutRounding="True"
	Focusable="True"
	ResizeMode="CanMinimize"
	WindowStartupLocation="CenterScreen"
	FontFamily="Segoe UI" xmlns:predatorsense="clr-namespace:PredatorSense"
	Background="{x:Null}"
	StateChanged="Window_StateChanged"
	Loaded="Window_Loaded"
	MouseLeftButtonDown="mainWindow_MouseLeftButtonDown"
	SourceInitialized="mainWindow_SourceInitialized"
	Closing="mainWindow_Closing">
	<FrameworkElement.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary
					Source="Style\100\PSStyle.xaml" />
				<ResourceDictionary
					Source="Lang\en.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</FrameworkElement.Resources>
	<Viewbox
		x:Name="wndViewbox"
		Stretch="Uniform">
		<Grid
			x:Name="Main_Grid"
			Width="1320"
			Height="760"
			MouseDown="Main_Grid_MouseDown">
			<Panel.Background>
				<ImageBrush
					ImageSource="{DynamicResource Img_bg}" />
			</Panel.Background>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="92" />
				<RowDefinition
					Height="*" />
				<RowDefinition
					Height="44" />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="84" />
				<ColumnDefinition
					Width="*" />
				<ColumnDefinition
					Width="84" />
			</Grid.ColumnDefinitions>
			<Image
				Grid.Column="1"
				Grid.Row="1"
				Width="1152"
				Height="624"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Source="{DynamicResource img_bg_newboard_oc}" />
			<Rectangle
				x:Name="MainTitle_Rectangle"
				Grid.Column="0"
				Grid.Row="0"
				Width="1320"
				Height="92"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Fill="#00FFFFFF"
				Focusable="False"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}" />
			<Button
				x:Name="Close_Button"
				Grid.Column="0"
				Grid.Row="0"
				Width="26"
				Height="20"
				HorizontalAlignment="Right"
				VerticalAlignment="Center"
				Margin="0,42,85,0"
				Template="{DynamicResource CloseButton_ControlTemplate}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}"
				Click="Close_Button_Click" />
			<Button
				x:Name="Minimize_Button"
				Grid.Column="0"
				Grid.Row="0"
				Width="28"
				Height="20"
				HorizontalAlignment="Right"
				VerticalAlignment="Center"
				Margin="0,42,126,0"
				Template="{DynamicResource MinimizeButton_ControlTemplate}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}"
				Click="Minimize_Button_Click" />
			<Button
				x:Name="Setting_Button"
				Grid.Column="0"
				Grid.Row="0"
				Width="26"
				Height="26"
				HorizontalAlignment="Right"
				VerticalAlignment="Center"
				Margin="0,42,169,0"
				Template="{DynamicResource SettingButton_ControlTemplate}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}"
				Click="Setting_Button_Click" />
			<Button
				x:Name="gfe_Button"
				Grid.Column="0"
				Grid.Row="0"
				Width="104"
				Height="28"
				HorizontalAlignment="Right"
				VerticalAlignment="Center"
				Margin="0,42,210,0"
				Template="{DynamicResource GFE_Button_ControlTemplate}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}"
				Click="gfe_Button_Click"
				Initialized="gfe_Button_Initialized"
				MouseEnter="gfe_Button_MouseEnter"
				MouseLeave="gfe_Button_MouseLeave" />
			<Grid
				x:Name="Content_Grid"
				Column="1"
				Row="1"
				HorizontalAlignment="Left"
				VerticalAlignment="Top">
				<Grid.RowDefinitions>
					<RowDefinition
						Height="*" />
					<RowDefinition
						Height="*" />
				</Grid.RowDefinitions>
			</Grid>
			<TextBlock
				x:Name="OC_text"
				Grid.Row="0"
				Grid.Column="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				FontSize="16"
				Foreground="#FFFFFF"
				Text="{DynamicResource MUI_Overclocking}"
				Margin="128,388,0,0"
				Grid.RowSpan="{Binding Path=RowDefinitions.Count, ElementName=Main_Grid}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}" />
			<TextBlock
				Grid.Row="0"
				Grid.Column="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				FontSize="16"
				Foreground="#FFFFFF"
				Text="{DynamicResource MUI_Monitoring}"
				Margin="400,388,0,0"
				Grid.RowSpan="{Binding Path=RowDefinitions.Count, ElementName=Main_Grid}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}" />
			<Popup
				x:Name="setting_Popup"
				Grid.Column="0"
				Grid.Row="0"
				IsOpen="False"
				StaysOpen="False"
				Placement="Bottom"
				HorizontalOffset="-231"
				VerticalOffset="5"
				AllowsTransparency="True"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}"
				PlacementTarget="{Binding ElementName=Setting_Button}"
				Closed="setting_Popup_Closed"
				Opened="setting_Popup_Opened">
				<Canvas
					Height="152"
					Width="340"
					ClipToBounds="True"
					HorizontalAlignment="Left"
					VerticalAlignment="Top">
					<Path
						Width="340"
						Height="152"
						Canvas.Left="0"
						Canvas.Top="0"
						Stretch="Fill"
						Fill="#FFDADADA"
						Data="F1 M3.00006,16.0137 L241.003,16.0137 L247.005,3.99994 L253.007,16.0137 L343,16.0137 L343,156.014 L3.00006,156.014 L3.00006,16.0137 Z">
						<UIElement.Effect>
							<DropShadowEffect
								BlurRadius="6"
								ShadowDepth="0"
								Opacity="0.599998"
								Color="#FF000000"
								Direction="315" />
						</UIElement.Effect>
					</Path>
					<Grid
						Width="340"
						Height="188">
						<Grid.RowDefinitions>
							<RowDefinition
								Height="20" />
							<RowDefinition
								Height="23" />
							<RowDefinition
								Height="*" />
							<RowDefinition
								Height="*" />
							<RowDefinition
								Height="*" />
							<RowDefinition
								Height="*" />
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition
								Width="15" />
							<ColumnDefinition
								Width="*" />
							<ColumnDefinition
								Width="10" />
						</Grid.ColumnDefinitions>
						<Border
							Grid.Column="1"
							Grid.Row="2"
							BorderThickness="0,0,0,1"
							BorderBrush="#999999" />
						<Border
							Grid.Column="1"
							Grid.Row="3"
							BorderThickness="0,0,0,1"
							BorderBrush="#999999" />
						<Border
							Grid.Column="1"
							Grid.Row="4"
							BorderThickness="0,0,0,1"
							BorderBrush="#999999" />
						<TextBlock
							Grid.Column="1"
							Grid.Row="1"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Text="{DynamicResource MUI_Advanced_Settings}"
							FontSize="14"
							FontWeight="Bold" />
						<TextBlock
							Grid.Column="1"
							Grid.Row="2"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Text="{DynamicResource MUI_Sticky_Key}"
							FontSize="14" />
						<TextBlock
							Grid.Column="1"
							Grid.Row="3"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Text="{DynamicResource MUI_Win_And_Menu_Key}"
							FontSize="14" />
						<TextBlock
							Grid.Column="1"
							Grid.Row="4"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Text="{DynamicResource MUI_Unit_Temperature}"
							FontSize="14" />
						<CheckBox
							x:Name="stickykey_Checkbox"
							Grid.Column="1"
							Grid.Row="2"
							Width="52"
							Height="20"
							HorizontalAlignment="Right"
							VerticalAlignment="Center"
							Template="{DynamicResource SettingCheckBox_ControlTemplate}"
							Checked="stickykey_Checkbox_Checked"
							Unchecked="stickykey_Checkbox_Unchecked" />
						<CheckBox
							x:Name="winmenukey_Checkbox"
							Grid.Column="1"
							Grid.Row="3"
							Width="52"
							Height="20"
							HorizontalAlignment="Right"
							VerticalAlignment="Center"
							Template="{DynamicResource SettingCheckBox_ControlTemplate}"
							Checked="winmenukey_Checkbox_Checked"
							Unchecked="winmenukey_Checkbox_Unchecked" />
						<StackPanel
							Grid.Column="1"
							Grid.Row="4"
							Width="Auto"
							Height="Auto"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Orientation="Horizontal"
							Margin="204,0,0,0">
							<RadioButton
								x:Name="C_RadioButton"
								Width="28"
								Height="20"
								GroupName="temperature"
								HorizontalAlignment="Left"
								VerticalAlignment="Center"
								Style="{DynamicResource SettingRadioButton_Style}"
								Checked="C_RadioButton_Checked" />
							<TextBlock
								x:Name="advance_C_TextBlock"
								Grid.Column="1"
								Grid.Row="4"
								HorizontalAlignment="Left"
								VerticalAlignment="Center"
								Margin="1,0,0,0"
								Text="˚C"
								FontSize="16"
								Foreground="#808080" />
						</StackPanel>
						<StackPanel
							Grid.Column="1"
							Grid.Row="4"
							Width="Auto"
							Height="Auto"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Orientation="Horizontal"
							Margin="268,0,0,0">
							<RadioButton
								x:Name="F_RadioButton"
								Width="28"
								Height="20"
								GroupName="temperature"
								HorizontalAlignment="Left"
								VerticalAlignment="Center"
								Style="{DynamicResource SettingRadioButton_Style}"
								Checked="F_RadioButton_Checked" />
							<TextBlock
								x:Name="advance_F_TextBlock"
								Grid.Column="1"
								Grid.Row="4"
								HorizontalAlignment="Left"
								VerticalAlignment="Center"
								Margin="1,0,0,0"
								Text="˚F"
								FontSize="16"
								Foreground="#808080" />
						</StackPanel>
					</Grid>
				</Canvas>
			</Popup>
			<Rectangle
				x:Name="Mask_Rectangle"
				Grid.Column="0"
				Grid.Row="0"
				Width="1320"
				Height="760"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Fill="#00FFFFFF"
				Visibility="Hidden"
				Panel.ZIndex="2"
				Grid.RowSpan="{Binding Path=RowDefinitions.Count, ElementName=Main_Grid}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}" />
			<Rectangle
				x:Name="main_mask_Rectangle"
				Grid.Column="0"
				Grid.Row="0"
				Width="1320"
				Height="760"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Fill="{DynamicResource main_mask_DrawingBrush}"
				Visibility="Hidden"
				Panel.ZIndex="2"
				Grid.RowSpan="{Binding Path=RowDefinitions.Count, ElementName=Main_Grid}"
				Grid.ColumnSpan="{Binding Path=ColumnDefinitions.Count, ElementName=Main_Grid}" />
		</Grid>
	</Viewbox>
</Window>
